<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.szgdemo">
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <uses-permission android:name="android.permission.READ_LOGS" />

    <application
        android:name="com.example.szgdemo.MyApp"
        android:allowBackup="true"
        android:icon="@drawable/dy_ic_app_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/dy_img_avatar_def"
        android:supportsRtl="true"
        android:theme="@style/Theme.Dylib">
        <activity
            android:name="com.example.szgdemo.MainActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.szgdemo.SplashActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>